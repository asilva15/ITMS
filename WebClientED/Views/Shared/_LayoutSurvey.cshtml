<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | Electrodata</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Plugins/css/star-rating.min.css" rel="stylesheet" />
</head>
<body>
    <style>
        .container{
            max-width:720px;
            font-family:"Segoe UI", Verdana, Helvetica, Sans-Serif;
            font-size:14px;
            font-weight:400;
        }
        .rostro {
            text-align:center;
            font-size:20px;
            cursor:pointer;
        }
        .rostro:hover {
            -webkit-transform: scale(1.25);
            -moz-transform: scale(1.25);
            -ms-transform: scale(1.25);
            -o-transform: scale(1.25);
            transform: scale(1.25);
            transition: all .5s ease-in-out;
        }
        #check1, #check3, #check5 {
            visibility:hidden;
        }
    </style>
    
    @*<div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
            </div>
        </div>
    </div>*@
    <div class="container">
        @RenderBody()
        <hr />
        <footer id="footerSurvey">
            <p>&copy; @DateTime.Now.Year - Electrodata SAC</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Plugins/js/star-rating.min.js"></script>
    @*<script src="~/Plugins/bootstrap-validator-master/dist/validator.min.js"></script>*@
    <script src="~/Scripts/Survey/Survey.js"></script>
</body>
</html>
