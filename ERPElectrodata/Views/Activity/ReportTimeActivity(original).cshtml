<script type="text/javascript">
    $(document).ready(function () {
        $('#SIN_DATE').datepicker({
            autoclose: true,
            startView: '-1d'
        });

        $('#TO_DATE').datepicker({
            autoclose: true,
            startView: '0d'
        });

        $("#ID_AREA").select2({
            placeholder: 'Seleccione...',
            minimumInputLength: 0,
            multiple: true,
            allowClear: true,
            ajax: {
                url: "/Activity/ListarAreaxOrganigrama",
                dataType: 'json',
                quietMillis: 100,
                data: function (params) {
                    return {
                        q: params.term,
                        page: params.page
                    };
                },
                processResults: function (data, page) {
                    return { results: data };
                },
            },
        });
    });
