@model ERPElectrodata.Models.TICKET

@{
    ViewBag.Title = "Create";
    //Layout = null;
}


<script src="~/Content/themes/plugin/toastr/toastr.min.js"></script>
<link href="~/Content/themes/plugin/toastr/toastr.min.css" rel="stylesheet" />

<div class="row">
    <div class="col-md-8">
        <div class="mb-3 card">
            <div class="card-header-tab card-header">Nueva Recepción de Activo</div>
            <div class="card-body">
                <div class="inBodyForm" id="Encabezado">
                    @using (Html.BeginForm("CreateRecepcion", "DeliveryReception", FormMethod.Post, new { enctype = "multipart/form-data", id = "FormReceptionAsset", target = "upload_target" }))
                    {
                        @Html.ValidationSummary(true)
                        <fieldset>
                            @Html.HiddenFor(model => model.ID_TYPE_TICK)
                            @Html.HiddenFor(model => model.ID_CATE)
                            @Html.HiddenFor(model => model.ID_PERS_ENTI_END)
                            @Html.HiddenFor(model => model.ID_PERS_ENTI_ASSI)
                            @Html.HiddenFor(model => model.ID_QUEU)
                            @Html.HiddenFor(model => model.ID_PRIO)
                            @Html.HiddenFor(model => model.ID_STAT)
                            @Html.HiddenFor(model => model.ID_AREA, new { @value = "0" })
                            @Html.HiddenFor(model => model.ID_SOUR)
                            <input type="hidden" id="Fecha" name="Fecha" value="@ViewBag.FEC_TICK" />

                            @Html.Hidden("nombresLicencias", null, new { id = "nombresLicencias" })
                            @Html.Hidden("idLicencias", null, new { id = "idLicencias" })

                            <div class="form-group row">
                                @if (Convert.ToInt32(Session["ID_ACCO"].ToString()) == 60)
                                {
                                    <div class="col-md-4">
                                        <div class="editor-label">
                                            Grupo <b style="color:red">(*)</b>
                                        </div>
                                        <div class="editor-field">
                                            @Html.TextBoxFor(model => model.IdGrupoActivo)
                                            @Html.ValidationMessageFor(model => model.IdGrupoActivo)
                                        </div>
                                    </div>
                                    @*<div class="col-md-4">
                                        <div class="editor-label">
                                            Unidad Minera <b style="color:red">(*)</b>
                                        </div>
                                        <div class="editor-field">
                                            @Html.TextBoxFor(model => model.ID_COMP)
                                            @Html.ValidationMessageFor(model => model.ID_COMP)
                                        </div>
                                    </div>*@
                                }
                                else if (Convert.ToInt32(Session["ID_ACCO"].ToString()) == 55)
                                {
                                    <input type="hidden" name="IdGrupoActivo" id="IdGrupoActivo" value="@ViewBag.IdGrupoIT" />
                                }
                                <div class="col-md-4">
                                    <div class="editor-label">
                                        Usuario <b style="color:red">(*)</b>
                                    </div>
                                    <div class="editor-field">
                                        @Html.TextBoxFor(model => model.ID_PERS_ENTI)
                                        @Html.ValidationMessageFor(model => model.ID_PERS_ENTI)
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="editor-label">
                                        Fecha de Entrega (mm/dd/yyyy) <b style="color:red">(*)</b>
                                    </div>
                                    <div class="editor-field">
                                        @Html.EditorFor(model => model.FEC_TICK)
                                        @Html.ValidationMessageFor(model => model.FEC_TICK)
                                    </div>
                                </div>
                                @{
                                    if (Convert.ToInt32(Session["ID_ACCO"].ToString()) != 56 && Convert.ToInt32(Session["ID_ACCO"].ToString()) != 57
                                        && Convert.ToInt32(Session["ID_ACCO"].ToString()) != 58 && Convert.ToInt32(Session["ID_ACCO"].ToString()) != 60)
                                    {
                                        <div class="col-md-4">
                                            <div class="editor-label">
                                                Locación <b style="color:red">(*)</b>
                                            </div>
                                            <div class="editor-field">
                                                @Html.TextBoxFor(model => model.ID_LOCA)
                                                @Html.ValidationMessageFor(model => model.ID_LOCA)
                                            </div>
                                        </div>
                                    }
                                }
                                @if (Convert.ToInt32(Session["ID_ACCO"].ToString()) == 60)
                                {
                                    <div class="col-md-4 divMicro">
                                        <div class="editor-label">
                                            Locación
                                        </div>
                                        <div class="editor-field">
                                            <input id="Locacion" name="Locacion" />
                                        </div>
                                    </div>
                                }
                            </div>
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <div class="editor-label">
                                        Comentario <b style="color:red">(*)</b>
                                    </div>
                                    <div class="editor-field">
                                        @Html.TextAreaFor(model => model.SUM_TICK)
                                        @Html.ValidationMessageFor(model => model.SUM_TICK)
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <input name="files" id="files" type="file" />
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <div id="divAssetsAdd" style="width: 100%;"></div>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-6">
                                </div>
                                <div class="col-md-3">
                                    <button id="reset" type="button" class="btn btn-light btn-block" onclick="cancel(); return false;">Limpiar</button>
                                </div>
                                <div class="col-md-3">
                                    <button type="submit" class="ladda-button mb-2 mr-2 btn btn-primary btn-block" data-style="expand-right" id="submit" onclick="enviarForm(this)">
                                        <span class="ladda-label">
                                            <i class="fa fa-save"></i> Guardar
                                        </span>
                                        <span class="ladda-spinner"></span>
                                        <div class="ladda-progress" style="width: 0px;"></div>
                                    </button>

                                    <style>
                                        .ladda-button.disabled {
                                            pointer-events: none;
                                        }
                                    </style>
                                </div>
                            </div>
                            <iframe id="upload_target" name="upload_target" src="" style="display:none;"></iframe>

                            <input id="idsActivo" type="hidden" name="idsActivo" value="" />
                            <input id="idsCondicion" type="hidden" name="idsCondicion" value="" />
                            <input id="idsNombreActivo" type="hidden" name="idsNombreActivo" value="" />
                            <input id="idsLicencias" type="hidden" name="idsLicencias" value="" />
                        </fieldset>
                    }
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-4">
        <div class="mb-3 card">
            <div class="card-header-tab card-header">Agregar Activo</div>
            <div class="card-body">
                <fieldset>
                    <div class="form-group row" id="divUsuariosCesados" style="display: none;">
                        <div class="col-md-12">
                            <div class="editor-label">
                                Usuarios Cesados
                            </div>
                            <div class="k-check">
                                <input id='chkUCesados' type='checkbox' name='chkUCesados'>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="col-md-12">
                            <div id="divUsuario" style="width:100%">
                            </div>
                            <br />

                            <div id="divUsuario2" style="width:100%">
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="col-md-12">
                            <div class="editor-label">
                                Buscar Activo
                            </div>
                            <div class="k-textbox" style="width:100%">
                                <input id='txtActivo' type='text' name='ID_ASSE'>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        @if (Convert.ToInt32(Session["ID_ACCO"].ToString()) != 60)
                        {
                            <div class="col-md-7">
                                <div class="editor-label">
                                    Tipo Activo
                                </div>
                                <div class="editor-field">
                                    <input id='cmbTipoActivo' type='text' name='cmbTipoActivo'>
                                </div>
                            </div>
                        }
                        <div class="col-md-5">
                            <br />
                            <button id='AgregarTodo' class='btn btn-primary btn-block'><i class="fa fa-upload"></i> Agregar todo</button>
                        </div>
                    </div>
                    <div style="overflow-y:auto; height:360px; width:100%;">
                        <div id="gridActivos">
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
    </div>

</div>

