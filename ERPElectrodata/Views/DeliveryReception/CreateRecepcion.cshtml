                        }
                    }
                },
                autoSync: true,
                serverFiltering: true,
                serverPaging: true,
                pageSize: 15,
                schema: {
                    data: "Data",
                }
            });

            dataSource.fetch(function () {
                var data = dataSource.data();
                console.log(data);
            });

            $("#gridActivos").kendoListView({
