@model List<ERPElectrodata.Models.ListarGruposPorCuenta_Result>
<ul class="list-group">
    @foreach (var grupo in Model)
    {
        <li class="list-group-item">
            <div class="widget-content p-0">
                <div class="widget-content-wrapper">
                    <div class="widget-content-left">
                        <div class="widget-heading">@grupo.NombreCompletoGrupo</div>
                        <div class="widget-subheading">@grupo.NombreGrupo</div>
                    </div>
                    <div class="widget-content-right">
                        <button class="btn-pill btn-hover-shine btn  btn-sm btn-danger" onclick="eliminarDesdeLista(@grupo.IdGrupo)">Eliminar</button>
                    </div>
                </div>
            </div>
        </li>
    }
</ul>
