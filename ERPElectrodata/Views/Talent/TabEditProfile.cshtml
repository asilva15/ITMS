                                <div class="editor-field" style="width:100%;">
                                    @Html.EditorFor(model => model.ID_NATI)
                                    @Html.ValidationMessageFor(model => model.ID_NATI)
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="editor-label">
                                    @ResourceLanguaje.Resource.Number @ResourceLanguaje.Resource.DocumentIdentityTalent
                                </div>
                                <div class="k-textbox" style="width:100%;">
                                    @Html.EditorFor(model => model.NUM_TYPE_DI)
                                    @Html.ValidationMessageFor(model => model.NUM_TYPE_DI)
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="editor-label">
                                    @ResourceLanguaje.Resource.Type @ResourceLanguaje.Resource.DocumentIdentityTalent
