                                <div class="editor-field" style="width:100%;">
                                    @Html.EditorFor(model => model.ID_TYPE_DI)
                                    @Html.ValidationMessageFor(model => model.ID_TYPE_DI)
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-3">
                                <div class="editor-label">
