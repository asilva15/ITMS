                    read: "/Area/ListByClient?var=" + Math.random()
                }
            }
        }).data("kendoComboBox");



        var IdProfesion = $("#IdProfesion_" + i).kendoComboBox({
            dataTextField: "Nombre",
            dataValueField: "Id",
            filter: "contains",
            autoBind: true,
            delay: 500,
            minLength: 0,
            suggest: true,
            dataSource: {
                serverFiltering: false,
                schema: {
                    data: "Data",
                    total: "Count"
                },
                transport: {
                    read: "/Profesion/Listar?var=" + Math.random()
                }
            }
        }).data("kendoComboBox");
        IdProfesion.bind("change", function (e) {
            if (IdProfesion.dataItem() === undefined) {
                $("#divEditarCombo_"+ i).css("display", "none");
            }
            else {
                var combobox = $("#IdProfesion_" + i).data("kendoComboBox");
                combobox.value(IdProfesion.dataItem().Id);
                $("#divEditarCombo_" + i).css("display", "block");
            }
        });

        var IdUniversidad = $("#IdUniversidad_" + i).kendoComboBox({
            dataTextField: "Nombre",
            dataValueField: "Id",
            filter: "contains",
            autoBind: true,
            delay: 500,
            minLength: 0,
            suggest: true,
            dataSource: {
                serverFiltering: false,
                schema: {
                    data: "Data",
                    total: "Count"
                },
                transport: {
                    read: "/Talent/ListarUniversidades?var=" + Math.random()
                }
            }
        }).data("kendoComboBox");

        $("#filesDocs_" + i).kendoUpload({
        localization: {
            select: "@ResourceLanguaje.Resource.AttachTalent @ResourceLanguaje.Resource.ContractTalent"
        },
        select: onSelectDocs,
        multiple: false,
    });
    }

    var onSelectDocs = function (e) {
        $.each(e.files, function (index, value) {
            if (value.extension.toLowerCase() != ".pdf") {
                e.preventDefault();
                toastr.warning("@ResourceLanguaje.Resource.AssetMessage8","@ResourceLanguaje.Resource.WrongFileExtension");
            }
        });
    };

    function NuevaCarrera() {
        $("#lblTitulo").text("Nueva Carrera");

        $(".modal-dialog").removeClass("modal-sm");
        $(".modal-dialog").removeClass("modal-sm");
        $(".modal-dialog").addClass("modal-sm");

        $('#modal-content').empty();
        $('#modal-content').load('/Profesion/Crear/');
    }

    function NuevaUniversidad(i) {
        $("#lblTitulo").text("Nueva Universidad");

        $(".modal-dialog").removeClass("modal-sm");
        $(".modal-dialog").removeClass("modal-md");
        $(".modal-dialog").addClass("modal-sm");

        $('#modal-content').empty();
        $('#modal-content').load('/Talent/CrearUniversidad?index=' + i + '&cantidad=' + modelCount);
    }

    function EditarCarrera(index) {
        var combobox = $("#IdProfesion" + index).data("kendoComboBox");
        if (a_entero(combobox.value()) > 0) {
            var htmlForm = '<div id="FormEditarCarrera"></div>';
            winFormPopUpModal("Editar Carrera", htmlForm, 250, 150);
            $("#FormEditarCarrera").load("/Profesion/Editar?id=" + combobox.value() + "&var=" + Math.random());
        }
        else { $("#divEditarCombo_" + index).css("display", "none"); }
    }

    if ('@ViewBag.MsgError' != "") {
        console.log("@ViewBag.MsgError");
    }

</script>