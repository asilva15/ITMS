<script type="text/javascript">

    $("#btnSubmitAll").on("click", function () {
        var $formularios = $("form.formSubmit");

        async function enviarAllFormularios() {
            for (let i = 0; i < $formularios.length; i++) {
                await enviarFormulario(i);
                await esperar(2000);
            }
        }

        function enviarFormulario(i) {
            return new Promise((resolve, reject) => {
                var formData = $($formularios[i]).serialize();
                toastr.info('Espere por favor...');

                $.ajax({
                    url: $formularios[i].action,
                    type: $formularios[i].method,
                    data: formData,
                    success: function (result) {
                        if (result.success) {
                            toastr.success('Colaborador ' + (i + 1) + ': ' + result.message);
                            resolve();
                        } else {
                            toastr.error('Colaborador ' + (i + 1) + ': ' + result.message);
                            resolve();
                        }
                    },
                    error: function (xhr, status, error) {
                        toastr.error('Ha ocurrido un error, contacte al administrador');
                        resolve();
                    }
                });
            });
        }

        function esperar(tiempo) {
            return new Promise(resolve => setTimeout(resolve, tiempo));
        }

        enviarAllFormularios();
    });

    $(".btnSubmitIndv").on("click", function (e) {
        e.stopPropagation();
        toastr.info('Espere por favor...');
        var $formulario = $(this).closest("form");

        $.ajax({
            url: $formulario.attr('action'),
            type: $formulario.attr('method'),
            data: $formulario.serialize(),
            success: function (result) {
                if (result.success) {
                    toastr.success(result.message);
                } else {
                    toastr.error(result.message);
                }
            },
            error: function (xhr, status, error) {
                toastr.error('Ha ocurrido un error, contacte al administrador');
            }
        });
    });


