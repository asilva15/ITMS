@model ERPElectrodata.Models.QUESTION

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create</h2>

@using (Html.BeginForm("SaveCreate", "Question", FormMethod.Post, new { id = "idFormQuest", target = "upload_target" }))
{
   
    <fieldset>
        <legend>QUESTION</legend>

        

        <div class="editor-label">
            @Html.Label("QUESTION NAME") 
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NAM_QUES)
            @Html.ValidationMessageFor(model => model.NAM_QUES)
        </div>

        <div class="editor-label">
            @Html.Label("DESCRIPTION NAME") 
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.DES_QUES)
            @Html.ValidationMessageFor(model => model.DES_QUES)
        </div>

        <div class="editor-label">
            @Html.Label("SATATE QUESTION")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.STA_QUES)
            @Html.ValidationMessageFor(model => model.STA_QUES)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>


<div>
    @Html.ActionLink("Back to List", "Index")
</div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
