@{
    Layout = null;
}
<style>
    b {
        color:#F83C50;
    }
</style>
<div class="box">
    <form id="Parametro">

        <div class="box-body">
            <div id="msjAlerta"></div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtFechaCreacionInicio">Fecha Inicio - Creación OP </label>
                        <input type="text" class="form-control pull-right" id="dtFechaCreacionInicio">                         
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtFechaCreacionFin">Fecha Fin - Creación OP </label>
                        <input type="text" class="form-control pull-right" id="dtFechaCreacionFin">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtFinSoporteInicio">Fecha Inicio - Soporte OP</label>
                        <input type="text" class="form-control pull-right" id="dtFinSoporteInicio">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtFinSoporteFin">Fecha Fin - Soporte OP</label>
                        <input type="text" class="form-control pull-right" id="dtFinSoporteFin">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtMantenimientoInicio">Fecha Inicio - Mantenimiento </label>
                        <input type="text" class="form-control pull-right" id="dtMantenimientoInicio">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="dtMantenimientoFin">Fecha Fin - Mantenimiento</label>
                        <input type="text" class="form-control pull-right" id="dtMantenimientoFin">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="cbCliente">Cliente</label>
                        <select id="cbCliente" name="cbCliente" class="form-control select2" style="width:100%" ></select>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="cbClienteFinal">Cliente Final</label>
                        <select id="cbClienteFinal" name="cbClienteFinal" class="form-control select2" style="width:100%" ></select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="cbMarca">Marca</label>
                        <select id="cbMarca" name="cbMarca" class="form-control select2" style="width:100%" ></select>
                    </div>
                </div>
                <div class="col-sm-3">
                    <label for="txtOP">OP</label>
                    <input id="txtOP" name="txtOP" class="form-control" type="text" />
                </div>

                <div class="col-sm-2">
                    <div class="form-group">
                        <label><br /></label>
                        <button id="btnBuscar" type="button" class="ladda-button mb-2 mr-2 btn btn-warning btn-block" data-style="expand-right" onclick="GenerateReport();">
                            <span class="ladda-label">
                                <i class="fa fa-search"></i> Buscar
                            </span>
                            <span class="ladda-spinner">
                            </span><div class="ladda-progress" style="width: 0px;"></div>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
<div class="box" style="margin-top:10px;">
    <div class="box-body">
        <iframe id="RSSoportesDet" style="height:570px; width:100%;border: 0px #eee solid;"></iframe>
    </div>
</div>  

