    }

    function MouseOverMenu(id) {
        $("#" + id).css("background-color", "#B21E1E");
    }
    function MouseOutMenu(id, menu) {
        if (IdSelect != menu) {
            $("#" + id).css("background-color", "#516882");
        }
    }

    function reloj(hh, min, seg) {
        seg = seg + 1;
        if (seg == 60) {
            min = min + 1;
            seg = 0;
            if (min == 60) {
                hh = hh + 1;
                min = 0;
                if (hh == 24) { hh = 0; }
            }
        }
        st = (hh < 10 ? "0" + hh : hh) + ":" + (min < 10 ? "0" + min : min);
        $("#divHora").html(st);
        setTimeout("reloj(" + hh + "," + min + "," + seg + ");", 1000);
    }

    var con = 1;
    var nameImage = '';
    var msnEti = '';
    var changeRate = 5000;
    function ChangeImage() {
        if (con == 1) {
