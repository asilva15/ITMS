        display = ancho / alto;

        $.ajax({
            url: "/Account/DateTimeServer",
            data: "var=" + Math.random(),
            type: "GET",
            dataType: "text",
            success: function (source) {
                data = source;
                var elem = source.split('|');
                $("#divDia").html(elem[0]);
                $("#divMonth").html(elem[4]);
                $("#divNameDia").html(elem[5]);
                reloj(parseInt(elem[1]), parseInt(elem[2]), parseInt(elem[3]));
            },
            error: function (dato) {
                alert("ERROR");
            }
        });

    });

    window.onload = function () {
        $("#BannerInferior").fadeOut(10, "swing", function () {
            $("#BannerInferior").css("display", "none");
        });
        window.onscroll = function () {
            var doc = document.body,
            scrollPosition = doc.scrollTop,
            pageSize = (doc.scrollHeight - doc.clientHeight),
            percentageScrolled = Math.floor((scrollPosition / pageSize) * 100);

            if (percentageScrolled >= 90) { // if the percentage is >= 50, scroll to top
                //window.scrollTo(0, 0);
                $("#BannerInferior").fadeIn(1500, "swing", function () {
                    $("#BannerInferior").show(500, "swing");
                    $("#BannerInferior").css("display", "block");
                });
            }
            else {
                $("#BannerInferior").fadeOut(10, "swing", function () {
                    $("#BannerInferior").css("display", "none");
                });
            }
        };
    };

    function MouseOver() {
        $("#divImage").css("background-image", "url('/Content/Images/flecha1.gif'");
