@model ERPElectrodata.Models.CLASS_ENTITY

@{
    Layout = null;
}
<script>
    $(document).ready(function () {
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
            increaseArea: '20%' // optional
        });
    });
</script>


<div id="FormEditarCompania">
    <div class="form-row" id="divContainerNewTicket">
        <div class="col-md-12">
            <div id="divNuevaLicencia">
                <!--Declaración de Formulario-->
                @using (Html.BeginForm("EditaCompania", "Companias", FormMethod.Post, new { enctype = "multipart/form-data", id = "FormEditarCompania", target = "upload_target" }))
                {
                    @Html.HiddenFor(model => model.ID_ENTI)
                    @Html.HiddenFor(model => model.FIR_NAME)
                    @Html.HiddenFor(model => model.LAS_NAME)
                    @Html.HiddenFor(model => model.MOT_NAME)
                    @Html.HiddenFor(model => model.SEC_NAME)
                    @Html.HiddenFor(model => model.THI_NAME)
                    @Html.HiddenFor(model => model.SEX_ENTI)
                    @Html.HiddenFor(model => model.ID_TYPE_ENTI)
                    @Html.HiddenFor(model => model.ID_TYPE_DI)
                    @Html.HiddenFor(model => model.CEL_ENTI)
                    @Html.HiddenFor(model => model.RPM_ENTI)
                    @Html.HiddenFor(model => model.EXT_ENTI)
                    @Html.HiddenFor(model => model.EMA_ENTI)
                    @Html.HiddenFor(model => model.CREATED)
                    @Html.HiddenFor(model => model.ID_USER)
                    @Html.HiddenFor(model => model.UserId)
                    @Html.HiddenFor(model => model.ID_CIVI_STAT)
                    @Html.HiddenFor(model => model.BIRTHDAY)
                    @Html.HiddenFor(model => model.EXPIRE_DATE_DNI)
                    @Html.HiddenFor(model => model.EMISSION_DATE_DNI)
                    @Html.HiddenFor(model => model.CONTACT_NAME)
                    @Html.HiddenFor(model => model.CONTACT_PHONE)
                    @Html.HiddenFor(model => model.PHONE_HOUSE)
                    @Html.HiddenFor(model => model.ID_DEGR_INST)
                    @Html.HiddenFor(model => model.ID_BLOO_GROU)
                    @Html.HiddenFor(model => model.ID_NATI)
                    @Html.HiddenFor(model => model.ID_UBIG)
                    @Html.HiddenFor(model => model.NUM_ID_SIDIGE)
                    @Html.HiddenFor(model => model.LAS_ACCE)
                    @Html.HiddenFor(model => model.PEN_ACCE)
                    @Html.HiddenFor(model => model.ON_LINE)
                    @Html.HiddenFor(model => model.ID_FOTO)
                    @Html.HiddenFor(model => model.IdProfesion)

                    <fieldset>
                        @*<legend>Crear Solucion TI</legend>*@

                        <div class="form-group">

                            <div class="form-row">
                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Compañia <b>*</b>
                                    </div>
                                    <input id="COM_NAME" class="form-control" type="text" name="COM_NAME" value="@ViewBag.Compania">
                                </div>

                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Ruc <b>*</b>
                                    </div>
                                    <input id="NUM_TYPE_DI" class="form-control" type="text" name="NUM_TYPE_DI" value="@ViewBag.Ruc">
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="form-row">
                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Telefono <b>*</b>
                                    </div>
                                    <input id="TEL_ENTI" class="form-control" type="text" name="TEL_ENTI" value="@ViewBag.Telefono">
                                </div>

                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Direccion <b>*</b>
                                    </div>
                                    <input id="ADDRESS" class="form-control" type="text" name="ADDRESS" value="@ViewBag.Direccion">
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="form-row">
                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Id Sidige <b>*</b>
                                    </div>
                                    <input id="ID_SIDIGE" class="form-control" type="text" name="ID_SIDIGE" value="@ViewBag.CodRuc">
                                </div>

                                <div class="col-md-6">
                                    <div class="editor-label">
                                        Vigencia
                                    </div>
                                    <div class="mt-2">
                                        @Html.CheckBox("VIG_ENTI", "@ViewBag.Vigencia")
                                        @Html.ValidationMessageFor(model => model.VIG_ENTI)
                                    </div>
                                </div>
                            </div>
                        </div>

                        @*<div class="divFondoUno">
                            
                            <div style="display:inline-block; float:right; padding:0px 15px 15px 5px;">
                                <div style=" float:right; padding:0px 15px 0px 0px; ">
                                    <button id="reset" class="k-button" onclick="cancel(); return false;">Cancelar</button>
                                </div>
                                <div style=" float:right; padding:0px 15px 0px 0px; ">
                                    <button id="submit" class="k-button">Editar</button>
                                </div>
                                <iframe id="upload_target" name="upload_target" src="" style="width:0px;height:0px;border-width:0px;" class="display:none"></iframe>
                            </div>
                        </div>*@

                        <div class="card-footer">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-3 ml-auto">
                                        <button id="submit" class="btn btn-primary btn-block">Guardar</button>
                                    </div>
                                    <div class="col-md-3">
                                        <button id="btnCancelar" class="ladda-button mb-2 btn btn-gradient-light btn-block widthMax" data-style="expand-right" data-dismiss="modal">
                                            <span class="ladda-label">Cancelar</span><span class="ladda-spinner"></span>
                                        </button>
                                    </div>

                                </div>
                            </div>
                        </div>


                    </fieldset>
                }
            </div>
        </div>
    </div>
</div>

