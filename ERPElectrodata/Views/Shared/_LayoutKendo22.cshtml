    function ComentariosNotificaciones(resultado) {
        $("#notificationsBody").empty();

        if (parseInt(resultado.Count) > 0) {

            $.each(resultado.Data, function (index, value) {
                var etiquetas = /<(br|p|ul|li|h1|h2|h3|h4|strong|ol|span)[^>]*>/g;
                var texto = value.Comentario.replace(etiquetas, ' ');
                var palabras = texto.split(/\s+/);
                var primeras20Palabras = palabras.slice(0, 20).join(' ');

                $('#notificationsBody').append(`
                    <div class="vertical-timeline-item dot-danger vertical-timeline-element" onclick="CambiarEstadoNotificacion(${value.ID_TICK})" style="cursor:pointer">
                        <div>
                            <span class="vertical-timeline-element-icon bounce-in"></span>
                            <div class="vertical-timeline-element-content bounce-in">
                                <h4 class="timeline-title">
                                    <b style="color:#16aaff">${value.Codigo}</b> - <b>${value.Nombre}</b></br>
                                </h4>
                                <span style="font-size: 14px">${primeras20Palabras}...</span>
                                <span class="vertical-timeline-element-date"></span>
                            </div>
                        </div>
                    </div>
                `);
            });
        }
    }

    function CambiarEstadoNotificacion(idTick) {
        $.ajax({
            url: '/Ticket/CambiarEstadoNotificacion?idTick=' + idTick,
            type: 'POST',
            success: function (response) {
                if (response == 'OK') {
                    $("#lista").trigger("click");
                    var nuevaUrl = "@Url.Content("~/DetailsTicket/Index/")" + idTick;
                    window.open(nuevaUrl, '_blank');
                }
            }
        });
    }

    $("#listaVacaciones").click(function () {
        $('#detalleAlerta').empty();
        $.ajax({
            type: "GET",                                              // tipo de request que estamos generando
            url: '/Vacaciones/AlertasVacaciones',                   // URL al que vamos a hacer el pedido
            data: null,                                                // data es un arreglo JSON que contiene los parámetros que
            // van a ser recibidos por la función del servidor
            contentType: "application/json; charset=utf-8",            // tipo de contenido
            dataType: "json",                                          // formato de transmición de datos
            async: true,                                               // si es asincrónico o no
            success: function (resultado) {                            // función que va a ejecutar si el pedido fue exitoso
                $.each(resultado.Data, function (index, value) {

                    $('#detalleAlerta').append('<div class="vertical-timeline-item dot-warning vertical-timeline-element">' +
                        '<div>' +
                        '<span class="vertical-timeline-element-icon bounce-in"></span>' +
                        '<div class="vertical-timeline-element-content bounce-in">' +
                        '<h4 class="timeline-title">' +
                        '<b style="color:#4a3a15">PENDIENTE: </b>' + value.Mensaje +
                        '</h4>' +
                        '<span class="vertical-timeline-element-date"></span>' +
                        '</div>' +
                        '</div>' +
                        '</div>');
                });

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) { // función que va a ejecutar si hubo algún tipo de error en el pedido
                var error = eval("(" + XMLHttpRequest.responseText + ")");
                aler(error.Message);
            }
        });
    });

    $(document).click(function () {
        //$("#notificationContainer").hide();
        $("#notificationsBody").empty();

    });
    function AprobarModal(IdInformeEDDetalle, opcion) {

        var id = 0;

        if (opcion == 1) {
            $("#lblTituloAprobarInforme").text("Módulo de Aprobación de Informe - " + IdInformeEDDetalle);

            $('#modal-contentAprobarInforme').empty();
            $('#modal-contentAprobarInforme').load('/InformeED/AprobarInforme?OP=' + id + '&IDI=' + IdInformeEDDetalle);

        } else if (opcion == 2) {
            $("#lblTituloAprobarInforme").text("Módulo de Corrección de Informe - " + IdInformeEDDetalle);

            $('#modal-contentAprobarInforme').empty();
            $('#modal-contentAprobarInforme').load('/InformeED/CorregirInforme?IDI=' + IdInformeEDDetalle);
        } else {
            $("#lblTituloMaquinaEstados").text("Pendiente Envio");
            $('#modal-contentMaquinaEstados').empty();
            $('#modal-contentMaquinaEstados').load('/InformeED/PendienteEnvioModal?OP=' + id + '&IDI=' + IdInformeEDDetalle + '&Estado=' + 5);
        }


    };
</script>


