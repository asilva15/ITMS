    <nav id="w2">
        <ul id="w3">
            <li><a onclick="OcultarTalent()" href="/Certificado/ProgramacionCertificado"><div class="inLink">Programación Certificado</div></a></li>
            <li><a onclick="OcultarTalent()" href="/Certificado/MisProgramaciones"><div class="inLink">Mis Programaciones</div></a></li>
            <li><a onclick="OcultarTalent()" href="/Certificado/MisCertificaciones"><div class="inLink">Mis Certificados</div></a></li>
        </ul>
    </nav>

    <h1>Gestión de Personas</h1>
    <hr />
    <nav id="w2">
        <ul id="w3">
            <li><a onclick="OcultarTalent()" href="Persona/Index"><div class="inLink">Lista de Personas</div></a></li>
        </ul>
    </nav>
</div>

<script type="text/javascript">

    function OcultarTalent() {
        $("#w1_t").removeClass("w1AnimOne");
    }

    function NewRequestVacation() {
        $("#w1_t").removeClass("w1AnimOne");

        $('#ModalContent').modal('show');
        $("#ModalContentAll").empty();
        $("#ModalContentAll").append('<div class="modal-header">' +
            '<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>' +
            '<h2 class="modal-title">New Request Vacation</h2>' +
            '</div>');
        $("#ModalContentAll").append("<div id='divFormNewRequest'></div>");
        $("#divFormNewRequest").load("/Vacation/RequestVacation?_=" + Math.random());
    }

</script>

