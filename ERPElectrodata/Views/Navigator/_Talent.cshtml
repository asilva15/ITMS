            {
                <li onclick="OcultarTalent()" id="liAssistance"><a href="Talent/ReporteGradoInstruccion" id="rptNivel"><div class="inLink">Nivel Colaborador</div></a></li>
            }
            <li onclick="OcultarTalent()" id="liSche"><a href="#/Scheduler" id="aSche"><div class="inLink">@ResourceLanguaje.Resource.CalendarTalent</div></a></li>
