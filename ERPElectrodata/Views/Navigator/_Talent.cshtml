@{
    Layout = null;
}

<div id="w1_t" class="menuVertical second w1">

    <a onclick="OcultarTalent()" style="text-decoration:none;color:white;" href="#/Talent"><h1>Talento</h1></a>
    <hr />
    <nav id="w2">
        <ul id="w3">
            <li onclick="OcultarTalent()"><a href="/Talent/Profile/@Session["ID_PERS_ENTI"]"><div class="inLink">@ResourceLanguaje.Resource.MyProfileTalent</div></a></li>
            @if (Session["RRHH"] != null && Convert.ToInt32(Session["RRHH"].ToString()) == 1)
            {
                <li onclick="OcultarTalent()"><a href="Talent/Create"><div class="inLink">@ResourceLanguaje.Resource.NewProfileTalent</div></a></li>
