            <li onclick="OcultarTalent()" style="display:none"><a href="#/EvaluationStaff" id="aEvalStaff"><div class="inLink">@ResourceLanguaje.Resource.EvaluationStaffTalent</div></a></li>
            @if ((Session["SUPERVISOR_SERVICEDESK"] != null && Convert.ToInt32(Session["SUPERVISOR_SERVICEDESK"].ToString()) == 1) || (Session["RRHH"] != null && Convert.ToInt32(Session["RRHH"].ToString()) == 1))
            {
                <li onclick="OcultarTalent()" id="liAssistance"><a href="#/Attendance" id="aAssistance"><div class="inLink">@ResourceLanguaje.Resource.AttendanceTalent</div></a></li>
            }
            @if (Session["RRHH"] != null && Convert.ToInt32(Session["RRHH"].ToString()) == 1)
            {

                <li onclick="OcultarTalent()" id="liChart"><a href="Talent/ViewChart"><div class="inLink">@ResourceLanguaje.Resource.OrganizationTalent</div></a></li>
                <li onclick="OcultarTalent()" id="liReport"><a href="Talent/Reports" id="aReport"><div class="inLink">@ResourceLanguaje.Resource.ReportTalent</div></a></li>
            }

            @if (Session["SUPERVISOR_RRHH"] != null && Convert.ToInt32(Session["SUPERVISOR_RRHH"].ToString()) == 1)
            {
                <li onclick="OcultarTalent()" id="liPayment"><a href="Talent/LoadPaymentBallots" id="aPayment"><div class="inLink">@ResourceLanguaje.Resource.PaymentBallotsTalent</div></a></li>
            }
            <li onclick="OcultarTalent()" id="liPayment"><a href="/Organigrama/Inicio" id="aOrg" target="_blank"><div class="inLink">Empresa</div></a></li>
            <li onclick="OcultarTalent()" id="liPayment"><a href="/Inventario/Buscar" id="aOrg" target="_blank"><div class="inLink">Postulaciones</div></a></li>
            @*@if (Session["SUPERVISOR_RRHH"] != null && Convert.ToInt32(Session["SUPERVISOR_RRHH"].ToString()) == 1)
                {
                    <li onclick="OcultarTalent()" id="liVacation"><a href="#/Vacation" id="aVacation"><div class="inLink">@ResourceLanguaje.Resource.VacationTalent</div></a></li>
                }
                <li style="display:none;" onclick="OcultarTalent()" id="liReport"><a href="#/Events" id="aReport"><div class="inLink">@ResourceLanguaje.Resource.EventTalent</div></a></li>
                <li><a onclick="NewRequestVacation(this)" href="#/RequestVacation"><div class="inLink">@ResourceLanguaje.Resource.RequestVacationTalent</div></a></li>*@

            @*<li onclick="SurveyEPS()" id="liAchie"><a href="#" id="aAchie"><div class="inLink">Encuesta EPS</div></a></li>
                <li onclick="TalentSurvey(this)" id="liReport"><a href="#" id="aReport"><div class="inLink">Survey</div></a></li>*@
        </ul>
    </nav>

    <h1>Evaluación Personal</h1>
    <hr />
    <nav id="w2">
        <ul id="w3">
            <li><a onclick="OcultarTalent()" href="EvaluacionPersonal/Index"><div class="inLink">Evaluación</div></a></li>
        </ul>
    </nav>
    <h1>Selección de Personal</h1>
    <hr />
    <nav id="w2">
        <ul id="w3">
            <li><a onclick="OcultarTalent()" href="/SeleccionSolicitudPersonal/CrearSolicitud"><div class="inLink">Nueva Solicitud</div></a></li>
            <li><a onclick="OcultarTalent()" href="/SeleccionSolicitudPersonal/ListarSolicitudPersonal"><div class="inLink">Mis Solicitudes</div></a></li>
            <li><a onclick="OcultarTalent()" href="/SeleccionSolicitudPersonal/ListarSolicitudesHistoricas"><div class="inLink">Solicitudes Históricas</div></a></li>
        </ul>
    </nav>
    <h1>Certificados</h1>
