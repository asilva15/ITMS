@model List<ERPElectrodata.Models.ListaUsuarioLocacion_Result>
<ul class="list-group">
    @foreach (var locacion in Model)
    {
        <li class="list-group-item">
            <div class="widget-content p-0">
                <div class="widget-content-wrapper">
                    <div class="widget-content-left">
                        <div class="widget-heading">@locacion.NAM_SITE</div>
                        <div class="widget-subheading">@locacion.NAM_LOCA</div>
                    </div>
                    <div class="widget-content-right">
                        <button class="btn-pill btn-hover-shine btn  btn-sm btn-danger" onclick="eliminarDesdeLista(@locacion.IdUsuarioLocacion)">Eliminar</button>
                    </div>
                </div>
            </div>
        </li>
    }
</ul>
