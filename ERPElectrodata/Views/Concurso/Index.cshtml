                },
                error: function (dato) {
                    alert("ERROR");
                }
            });
        });
        $("#id4").click(function (e) {
            $.ajax({
                url: "/Concurso/Registrar/4",
                data: "var=" + Math.random(),
                type: "GET",
                dataType: "text",
                success: function (source) {
                    popup();
                },
                error: function (dato) {
                    alert("ERROR");
                }
            });
        });
        $("#id5").click(function (e) {
            $.ajax({
                url: "/Concurso/Registrar/5",
