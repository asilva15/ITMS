<script language="javascript" type="text/javascript">
    $(document).ready(function () {

        function popup() {
            var title = 'Concurso Vive los Valores';
            var body = "<div style='border-bottom:1px dotted #dadada;padding:0px 0px 20px 0px;'>" +
                "<div style='padding:10px 0px 10px 0px;'>Gracias por participar.</div>" +
                "<div style='float:right; padding:30px 6px 0px 6px; font-size:0.9em;'>" +
                "<button id='btnContinuee' class='k-button'>Continue</button>" +
                       "</div></div>";

            winPopUpModal1(title, body, 400, 150);

            $("#btnContinuee").click(function () {
                $("#dlg").hide('200', "swing", function () {
                    $("#bkg").fadeOut("300");
                    window.open("https://www.facebook.com/Grupo-e-data-767481443318728/?fref=ts", "_self");
                });
            });
        }

        $("#id1").click(function (e) {
            $.ajax({
                url: "/Concurso/Registrar/1",
                data: "var=" + Math.random(),
                type: "GET",
                dataType: "text",
                success: function (source) {
                    popup();
                },
                error: function (dato) {
                    alert("ERROR");
                }
            });
        });
        $("#id2").click(function (e) {
            $.ajax({
                url: "/Concurso/Registrar/2",
                data: "var=" + Math.random(),
                type: "GET",
                dataType: "text",
                success: function (source) {
                    popup();
                },
                error: function (dato) {
                    alert("ERROR");
                }
            });
        });
        $("#id3").click(function (e) {
            $.ajax({
                url: "/Concurso/Registrar/3",
                data: "var=" + Math.random(),
                type: "GET",
                dataType: "text",
