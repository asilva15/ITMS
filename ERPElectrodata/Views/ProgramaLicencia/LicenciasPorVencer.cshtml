<div class="mb-3 card">
    <div class="card-header-tab card-header">
        LICENCIAS POR VENCER
    </div>
    <div class="card-body">
        <div class="row no-gutters align-items-center justify-content-center">
            <table class="table table-bordered table-striped" id="tbLicenciasPorVencer" style="width: 80vw">
                <thead>
                    <tr>
                        <th>Asignado a</th>
                        <th>Tipo de Activo</th>
                        <th>Nombre de Activo</th>
                        <th>Programa</th>
                        <th>Tipo de Licencia</th>
                        <th>Versión</th>
                        <th>Fecha de Vencimiento</th>
                        <th>Ver Detalle</th>
                    </tr>
                </thead>
                <tbody id="TablaLicencias"></tbody>
            </table>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        ObtenerDatosLicenciaPorVencer();
    });

    //Listar Licencias Por Vencer
    function ObtenerDatosLicenciaPorVencer() {

        $('#tbLicenciasPorVencer').dataTable().fnDestroy();

        $('#tbLicenciasPorVencer').DataTable({
            dom: 'Bfrtip',
            responsive: true,
            ajax: "/ProgramaLicencia/ListadoLicenciasPorVencer/",
            columns: [
                { data: "Nombre" },
                { data: "TipoActivo" },
                { data: "NombreActivo" },
                { data: "NombrePrograma" },
                { data: "TipoLicencia" },
                { data: "Version" },
                { data: "FechaVencimiento" },
                {
                    data: null,
                    render: function (data, type, row) {
                        return '<div style="text-align: center;">' +
                            '<a href="\\Asset/Detalle/' + data.ID_ASSE + '"><span id="' + data.IdPrograma + '" title="Ver Detalle" class="fa fa-eye" aria-hidden="true" style="color: #2D5C88; cursor: pointer; font-size:13px; "></span></a>' +
                            '</div>';
                    }
                },
            ],
            "language": {
                "lengthMenu": "Mostrar _MENU_ registros",
                "info": "Mostrando página _PAGE_ de _PAGES_ de _TOTAL_ registros",
                "infoEmpty": "No hay registros disponibles",
                "infoFiltered": "(filtrada de _MAX_ registros)",
                "loadingRecords": "Cargando...",
                "processing": "Procesando...",
                "search": "Buscar:",
                "zeroRecords": "No se encontraron registros coincidentes",
                "paginate": {
                    "next": "Siguiente",
                    "previous": "Anterior"
                },
            },
        });


    }


</script>
